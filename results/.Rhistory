matrix_cols <- function(i, data, len) {
vec <- rep(0, len)
df <- data[[i]]
for(j in 1:len) {
m <- match(j, unlist(df[,1]))
if(!is.na(m)) {
#print(m)
vec[j] <- df[m, 2]
}
}
#print(vec)
return(unlist(vec))
}
